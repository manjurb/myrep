<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"
	xmlns:context="http://www.springframework.org/schema/context">
	
	<bean name="item" class="com.majnu.spring.springcoreadvanced.autowiring.Item"
		 p:id="10" p:name="Bags" p:price="200.0" p:quantity="2"/>	
		 
	
	<bean name="cart" class="com.majnu.spring.springcoreadvanced.autowiring.ShoppingCart" 
	scope="prototype" autowire="constructor"/>
	
	
	
</beans>
